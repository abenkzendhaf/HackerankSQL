SELECT CITY FROM STATION
WHERE CITY NOT LIKE 'A%' AND CITY NOT LIKE '%A'
AND CITY NOT LIKE 'A%' AND CITY NOT LIKE '%I'
AND CITY NOT LIKE 'A%' AND CITY NOT LIKE '%U'
AND CITY NOT LIKE 'A%' AND CITY NOT LIKE '%E'
AND CITY NOT LIKE 'A% 'AND CITY NOT LIKE '%O'
AND CITY NOT LIKE 'I% 'AND CITY NOT LIKE '%A'
AND CITY NOT LIKE 'I% 'AND CITY NOT LIKE '%I'
AND CITY NOT LIKE 'I% 'AND CITY NOT LIKE '%U'
AND CITY NOT LIKE 'I% 'AND CITY NOT LIKE '%E'
AND CITY NOT LIKE 'I% 'AND CITY NOT LIKE '%O'
AND CITY NOT LIKE 'U%A'
AND CITY NOT LIKE 'U%I'
AND CITY NOT LIKE 'U%U'
AND CITY NOT LIKE 'U%E'
AND CITY NOT LIKE 'U%O'
AND CITY NOT LIKE 'E%A'
AND CITY NOT LIKE 'E%I'
AND CITY NOT LIKE 'E%U'
AND CITY NOT LIKE 'E%E'
AND CITY NOT LIKE 'E%O'
AND CITY NOT LIKE 'O%A'
AND CITY NOT LIKE 'O%I'
AND CITY NOT LIKE 'O%U'
AND CITY NOT LIKE 'O%E'
AND CITY NOT LIKE 'O%O'
GROUP BY CITY
ORDER BY CITY ASC
